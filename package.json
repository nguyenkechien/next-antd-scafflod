{
  "name": "base",
  "version": "1.0.0",
  "description": "Next.js.",
  "main": "server.js",
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "cjs": true,
  "mode": "auto",
  "scripts": {
    "start": "node -r esm server.js",
    "build": "next build",
    "prod": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "nextjs",
    "antd",
    "react",
    "ssr",
    "redux",
    "redux-saga"
  ],
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "antd": "^4.9.4",
    "babel-plugin-import": "^1.9.0",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.1",
    "dynamic-antd-theme": "^0.3.1",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "if-comp": "^0.0.8",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.13.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.13",
    "next": "^8.1.0",
    "next-nprogress": "^1.4.0",
    "next-redux-saga": "^3.0.0",
    "next-redux-wrapper": "^2.0.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.13.8",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/body-parser": "^1.19.0",
    "@types/core-js": "^2.5.4",
    "@types/express": "^4.17.9",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "babel-eslint": "^9.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.22.0",
    "esm": "^3.2.25",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.5",
    "styled-components": "^5.2.1",
    "terser-webpack-plugin": "^1.1.0",
    "thread-loader": "^2.1.3",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^3.3.2"
